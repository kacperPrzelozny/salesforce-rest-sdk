{
    "name": "kacperprzelozny/salesforce-rest-sdk",
    "description": "An SDK for the Salesforce Rest API",
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.3.8",
        "jms/serializer": "^3.30",
        "ramsey/uuid": "^4.7.6",
        "doctrine/collections": "^2.2",
        "symfony/cache": "^7",
        "psr/log": "^3",
        "symfony/expression-language": "^7",
        "jms/metadata": "^2.8"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Alex Boyce",
            "email": "alex.boyce@advisorsexcel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AE\\SalesforceRestSdk\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AE\\SalesforceRestSdk\\Tests\\": ["Tests/"]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
